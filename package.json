{
  "name": "uniswap-v2-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/graphprotocol/uniswap-v2-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-local": "graph create btcix/shuttleswap --node https://thegraph-node.shulik.xyz",
    "deploy-local": "graph deploy btcix/shuttleswap --debug --ipfs https://thegraph-ipfs.shulik.xyz --node https://thegraph-node.shulik.xyz",
    "deploy": "graph deploy btcix/shuttleswap --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-staging": "graph deploy $THE_GRAPH_GITHUB_USER/$THE_GRAPH_SUBGRAPH_NAME /Uniswap --ipfs https://thegraph-ipfs.shulik.xyz --node https://thegraph-node.shulik.xyz",
    "watch-local": "graph deploy btcix/shuttleswap --watch --debug --node https://thegraph-node.shulik.xyz --ipfs https://thegraph-ipfs.shulik.xyz"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
